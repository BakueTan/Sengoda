      <Query>
        <DataSourceName>soccerConnectionString</DataSourceName>
        <CommandText>SELECT        StudentLeaving.StudentID, enrollment.Status, StudentPersonalDetails.StudentName, enrollment.Intake, enrollment.[Date Joined], enrollment.Semester, 
                         enrollment.Year, enrollment.Program, StudentPersonalDetails.StudentSurname, StudentLeaving.DateLeft, StudentLeaving.Reason, enrollment.Session
FROM            StudentLeaving INNER JOIN
                         StudentPersonalDetails ON StudentLeaving.StudentID = StudentPersonalDetails.StudentID INNER JOIN
                         enrollment ON StudentLeaving.StudentID = enrollment.StudentID
WHERE        (DATEPART(yyyy, StudentLeaving.DateLeft) = @year) AND StudentLeaving.Reason  = @reason and (enrollment.semester   between @sem and @sem2)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
