  <DataSets>
    <DataSet Name="dsReports_KnowHow">
      <Fields>
        <Field Name="STUDENTS">
          <DataField>STUDENTS</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Religion">
          <DataField>Religion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Section">
          <DataField>Section</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>soccerConnectionString</DataSourceName>
        <CommandText>SELECT     COUNT(StudentPersonalDetails.StudentID) AS STUDENTS, StudentPersonalDetails.Religion, forms.Section, forms.Description
FROM         StudentPersonalDetails INNER JOIN
                      forms ON StudentPersonalDetails.Program = forms.Forms
WHERE     (DATEPART(yyyy, StudentPersonalDetails.Date_Enrolled) = @year)
GROUP BY StudentPersonalDetails.Religion, forms.Section, forms.Description</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
