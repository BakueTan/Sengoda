  <DataSets>
    <DataSet Name="dsReports_SujectsTaught">
      <Query>
        <DataSourceName>soccerConnectionString</DataSourceName>
        <CommandText>SELECT DISTINCT 
                         Questionnaire.school, Questionnaire.lecturer, Questionnaire.clas, Questionnaire.subject, Questionnaire.Semester, Questionnaire.intake, Questionnaire.Session, 
                         Questionnaire.Program, StaffPersonalDetails.Name, StaffPersonalDetails.Surname, StaffPersonalDetails.Gender, Subjects.Subject AS SubDesc
FROM            Questionnaire INNER JOIN
                         StaffPersonalDetails ON Questionnaire.lecturer = StaffPersonalDetails.staffID INNER JOIN
                         Subjects ON Questionnaire.subject = Subjects.SubjectID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
