      <Query>
        <DataSourceName>soccerConnectionString</DataSourceName>
        <CommandText>SELECT        Bookissues.borrower, Bookissues.dateissued, Bookissues.issueno, Bookissues.issuestate, bookissiue_lines.duedate, Bookissues.issuer, bookissiue_lines.book, vwUsers.StudentName, 
                         vwUsers.StudentSurname, BookMaster.title, BookMaster.Loan, @user AS LoggedUser, Bookissues.Active, Bookissues.barcode, Bookissues.issuecode
FROM            Bookissues INNER JOIN
                         bookissiue_lines ON Bookissues.issueno = bookissiue_lines.issueno INNER JOIN
                         vwUsers ON vwUsers.StudentID = Bookissues.borrower INNER JOIN
                         BookMaster ON bookissiue_lines.book = BookMaster.serial</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
