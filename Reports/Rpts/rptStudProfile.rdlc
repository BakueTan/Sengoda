      <Query>
        <DataSourceName>soccerConnectionString</DataSourceName>
        <CommandText>SELECT        FeesPayments_Header.StudentID, feespayments_details.Amount, StudentPersonalDetails.StudentName, StudentPersonalDetails.StudentSurname, enrollment.Intake,
                          feespayments_details.Period, FeesPayments_Header.Date, FeesPayments_Header.Receipt, enrollment.Program, enrollment.Year, enrollment.Semester, 
                         enrollment.[Date Joined], PaymentPeriods.Description AS Perioddesc, forms.Description, @period AS Pfrom, @period1 AS Pto, enrollment.Session, 
                         FeesPayments_Header.usr, feespayments_details.Cartegory, PaymentType.Type AS PayType, feespayments_details.line_number, 
                         FeesPayments_Header.CashType, @user AS logduser, FeesPayments_Header.PostDate, feespayments_details.Reference, feespayments_details.Biltype, 
                         StudentPersonalDetails.[Residential Adress], StudentPersonalDetails.[Contact Number], StudentPersonalDetails.EmailAddress, StudentPersonalDetails.Address2, 
                         StudentPersonalDetails.Address3, StudentPersonalDetails.Image, enrollment.Status, @info AS AddInfo, enrollment.Gender, FeesPayments_Header.type, 
                         schoolinfo.Name, schoolinfo.Web, schoolinfo.Address, schoolinfo.Logo, schoolinfo.Phone
FROM            FeesPayments_Header INNER JOIN
                         StudentPersonalDetails ON FeesPayments_Header.StudentID = StudentPersonalDetails.StudentID INNER JOIN
                         enrollment ON FeesPayments_Header.StudentID = enrollment.StudentID INNER JOIN
                         feespayments_details ON feespayments_details.Receipt = FeesPayments_Header.Receipt INNER JOIN
                         PaymentPeriods ON feespayments_details.Period = PaymentPeriods.Period INNER JOIN
                         forms ON enrollment.Program = forms.Forms INNER JOIN
                         PaymentType ON feespayments_details.Cartegory = PaymentType.Payment CROSS JOIN
                         schoolinfo
WHERE        (feespayments_details.Period BETWEEN @period AND @period1) AND (enrollment.Year BETWEEN @level1 AND @level2) AND (enrollment.Semester = @sem) AND 
                         (enrollment.[Date Joined] = @clas) AND (enrollment.Session BETWEEN @sess AND @sess2) AND (FeesPayments_Header.Status = '1') AND 
                         (enrollment.Status = N'Available' OR
                         enrollment.Status = N'Competed') AND (feespayments_details.Cartegory = @cart) OR
                         (feespayments_details.Period BETWEEN @period AND @period1) AND (enrollment.Year BETWEEN @level1 AND @level2) AND (enrollment.Semester = @sem) AND 
                         (enrollment.[Date Joined] = @clas) AND (enrollment.Session BETWEEN @sess AND @sess2) AND (FeesPayments_Header.Status = '1') AND 
                         (enrollment.Status = N'Available' OR
                         enrollment.Status = N'Competed') AND (feespayments_details.Biltype = @cart)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
