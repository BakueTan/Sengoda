      <Query>
        <DataSourceName>soccerConnectionString</DataSourceName>
        <CommandText>SELECT        vwLectAvgs.LectAvg, vwLectAvgs.lecturer, vwLectAvgs.intake, vwLectAvgs.clas, StaffPersonalDetails.Name, StaffPersonalDetails.Surname, vwLectAvgs.school, 
                         vwLectAvgs.Questionaires, vwLectAvgs.subjects
FROM            vwLectAvgs INNER JOIN
                         StaffPersonalDetails ON vwLectAvgs.lecturer = StaffPersonalDetails.staffID
ORDER BY vwLectAvgs.LectAvg DESC, vwLectAvgs.Questionaires DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
