      <Query>
        <DataSourceName>soccerConnectionString</DataSourceName>
        <CommandText>SELECT        StaffRegister.Date, StaffRegister.complete, StaffPersonalDetails.Name, StaffPersonalDetails.Surname, StaffPersonalDetails.StaffType, 
                         StaffPersonalDetails.Department, @date1 AS datefrom, @date2 AS DateTo, StaffRegister.fulldate_in, StaffRegister.fulldate_out, StaffRegister.hours, 
                         StaffPersonalDetails.staffID, StaffRegister.time_in, StaffRegister.time_out
FROM            StaffRegister FULL OUTER JOIN
                         StaffPersonalDetails ON StaffRegister.Staffid = StaffPersonalDetails.staffID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
