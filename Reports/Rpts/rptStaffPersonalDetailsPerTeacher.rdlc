      <Query>
        <DataSourceName>soccerConnectionString</DataSourceName>
        <CommandText>SELECT        StaffPersonalDetails.staffID, StaffPersonalDetails.Name, StaffPersonalDetails.Surname, StaffPersonalDetails.StaffType, StaffPersonalDetails.DateOfBirth, 
                         StaffPersonalDetails.PlaceOfBirth AS Salary, StaffPersonalDetails.IDNumber, StaffPersonalDetails.Religion AS ECNumber, StaffPersonalDetails.Gender, 
                         StaffPersonalDetails.[Residential Address], StaffPersonalDetails.Contact, StaffPersonalDetails.[Email adrress], StaffPersonalDetails.DateJoinedSchool, 
                         StaffPersonalDetails.nokname, StaffPersonalDetails.noksurname AS SubjectsTaught, StaffPersonalDetails.relation, StaffPersonalDetails.nokadd, 
                         StaffPersonalDetails.nokcontact, StaffPersonalDetails.Department AS Designation, StaffPersonalDetails.image, staffStatus.Status, 
                         StaffPersonalDetails.Contract
FROM            StaffPersonalDetails INNER JOIN
                         staffStatus ON StaffPersonalDetails.staffID = staffStatus.staffID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
